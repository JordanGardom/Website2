public class StringToIntExample1{
public static void main(String args[]){

String s="3804";

int i=Integer.parseInt(s);

System.out.println(i);
}}

public class StringToIntegerExample2{  
public static void main(String args[]){  
String s="200";  
Integer i=Integer.valueOf(s);  
System.out.println(i);  
}}





 